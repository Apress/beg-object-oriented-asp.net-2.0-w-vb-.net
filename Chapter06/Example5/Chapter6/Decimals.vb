Namespace Math
    Public Class Decimals
        Public Function AddDecimal(ByVal firstDecimal As Decimal, ByVal secondDecimal As Decimal) As Decimal
            Return firstDecimal + secondDecimal
        End Function
        Public Function MultiplyDecimal(ByVal firstDecimal As Decimal, ByVal secondDecimal As Decimal) As Decimal
            Return firstDecimal * secondDecimal
        End Function
    End Class
End Namespace
